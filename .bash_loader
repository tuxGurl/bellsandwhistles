#!/bin/bash

# Load in all the bash scrips 

#Preferences 
export EDITOR=vim

# Aliases
if [ -f ~/.bash_alias ]; then
    . ~/.bash_alias
fi

# Project Specific
if [ -f ~/.bash_prj ]; then
    . ~/.bash_prj
fi

#Show SVN details in prompt
if [ -f ~/.bash_svn ]; then
    . ~/.bash_svn
fi

# Svn Tools
export PATH=~svnx.sh:$PATH

if [ -f ~/.cygwin ]; then
    . ~/.cygwin
fi

